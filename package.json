{
  "name": "tallstreet-playground",
  "version": "1.0.0",
  "description": "A playground for react-js components.",
  "main": "app/client.js",
  "scripts": {
    "test": "mocha --recursive --require app/test.setup.js $(find app -name '*_test.js')",
    "test:watch": "npm test -- --watch",
    "lint": "node ./node_modules/eslint/bin/eslint.js -c .eslintrc app",
    "start-dev-backend": "NODE_PATH=\"./app\" NODE_ENV=\"development\" node ./babel.server",
    "test:ci": "npm test",
    "start-prod-backend": "NODE_PATH=\"./app\" NODE_ENV=\"production\" PORT=\"8080\" node ./babel.server",
    "build": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js -p",
    "build:gh": "npm run build && node ./build-gh-pages.js && rm -r ./dist",
    "start": "node ./devserver.js --DEBUG=true",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run start\" \"npm run start-dev-backend\""
  },
  "author": "Gary Roberts <contact@tallstreet.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "bootstrap-sass": "^3.3.5",
    "bourbon": "^4.2.5",
    "classnames": "^2.1.5",
    "concurrently": "^0.1.1",
    "css-loader": "^0.19.0",
    "cssnext-loader": "^1.0.1",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "0.1.0",
    "eslint-plugin-react": "^3.5.1",
    "expect": "^1.12.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.4.0",
    "html-webpack-plugin": "^1.6.1",
    "inuit-box": "^0.4.4",
    "inuit-box-sizing": "^0.2.0",
    "inuit-defaults": "^0.2.3",
    "inuit-functions": "^0.2.0",
    "inuit-images": "^0.3.3",
    "inuit-layout": "^0.2.2",
